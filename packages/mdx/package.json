{
  "name": "@bacons/mdx",
  "version": "0.4.4",
  "description": "Universal MDX renderer for React Native",
  "main": "build/index",
  "files": [
    "build",
    "metro-transformer.js",
    "metro-transformer.d.ts",
    "jsx",
    "metro.js",
    "default-metro-transformer.js"
  ],
